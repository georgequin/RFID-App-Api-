<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<HBox fx:id="centerPane" alignment="TOP_CENTER" prefHeight="728.0" prefWidth="786.0" style="-fx-background-color:#1c3c3b;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
        <VBox>
            <children>
                <Region />
                <HBox VBox.vgrow="NEVER">
                    <children>
                        <Label prefHeight="27.0" prefWidth="125.0" text="COURSE CODE" textAlignment="CENTER" textFill="#bfbfbf" HBox.hgrow="ALWAYS">
                            <font>
                                <Font name="System Bold" size="18.0" />
                            </font>
                            <HBox.margin>
                                <Insets right="10.0" />
                            </HBox.margin>
                        </Label>
                  <ComboBox fx:id="select_course" editable="true" prefHeight="25.0" prefWidth="176.0" promptText="ddzv " />
                    </children>
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="50.0" />
                    </VBox.margin>
                </HBox>
                <Button fx:id="start_Exams" mnemonicParsing="false" text="START EXAMINATION SCAN" textAlignment="CENTER">
                    <font>
                        <Font name="Baskerville Old Face" size="30.0" />
                    </font>
                    <textFill>
                        <Color red="0.110" green="0.235" blue="0.231" fx:id="x2" />
                    </textFill>
                </Button>
            </children>
        </VBox>
        <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
    </children>
</HBox>
