<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox minWidth="800.0" prefWidth="800.0" style="-fx-background-color: #c23426;"  xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox VBox.vgrow="ALWAYS">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </VBox.margin>
            <children>
                <HBox fx:id="green_pane" minWidth="-Infinity" prefWidth="800.0" style="-fx-background-color:#1c3c3b;" VBox.vgrow="ALWAYS">
                    <Region HBox.hgrow="ALWAYS" />
                    <VBox fx:id="error_pane" alignment="CENTER" prefHeight="1034.0" prefWidth="708.0" style="-fx-background-color: #c23426;" HBox.hgrow="ALWAYS">
                        <HBox prefHeight="100.0" prefWidth="200.0">
                            <Region HBox.hgrow="ALWAYS" />
                            <Label fx:id="sec" alignment="CENTER" contentDisplay="CENTER" text="SECURITY ALERT!" textAlignment="CENTER" textFill="WHITE" HBox.hgrow="ALWAYS">
                                <font>
                                    <Font name="Arial Black" size="30.0" />
                                </font>
                                <HBox.margin>
                                    <Insets top="20.0" fx:id="x2" />
                                </HBox.margin>
                            </Label>
                            <Region HBox.hgrow="ALWAYS" />
                        </HBox>
                        <Region VBox.vgrow="ALWAYS" />
                        <Label alignment="CENTER" text="!" textFill="#171616">
                            <font>
                                <Font name="Impact" size="96.0" />
                            </font>
                        </Label>
                        <HBox prefHeight="100.0" prefWidth="200.0">
                            <Region HBox.hgrow="ALWAYS" />
                            <Label text="CARD NOT REGISTERED!!!" textFill="WHITE" HBox.margin="$x2">
                                <font>
                                    <Font name="System Bold Italic" size="25.0" />
                                </font>
                            </Label>
                            <Region HBox.hgrow="ALWAYS" />
                        </HBox>
                        <Region prefHeight="19.0" prefWidth="662.0" VBox.vgrow="ALWAYS" />
                        <HBox.margin>
                            <Insets bottom="20.0" top="20.0" />
                        </HBox.margin>
                    </VBox>
                    <Region HBox.hgrow="ALWAYS" />
                </HBox>
            </children>
        </VBox>
    </children>
</VBox>
